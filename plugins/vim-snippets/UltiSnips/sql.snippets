snippet sel "Select from table" b
	select * from ${0:table}
endsnippet

snippet selt10 "Select top 10 from table" b
	select top 10 * from ${0:table}
endsnippet

snippet selt100 "Select top 100 from table" b
	select top 100 * from ${0:table}
endsnippet

snippet selw "Select from table where" b
	select * from ${0:table}
	where ${1:column} = '${2:value}'
endsnippet

snippet del "Delete from table where" b
	delete from ${0:table}
	where ${1:column} = '${2:value}'
endsnippet
